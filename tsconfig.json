{
    "compilerOptions": {
        "strict": true,
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "allowJs": false,
        "module": "CommonJS",
        "jsx": "react-native",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "experimentalDecorators": true,
        "useDefineForClassFields": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "lib": [
            "DOM",
            "ESNext"
        ],
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "target": "ESNext"
    },
    "files": [
        "index.ts"
    ],
    "exclude": [
        "node_modules",
    ]
}